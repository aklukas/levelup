  "scripts": {
    "ng": "ng",
    "start:woohoo": "ng serve --project=woohoo --port 4200",
    "deploy:woohoo": "ng build && cp ./dist/apps/woohoo/index.html ./dist/apps/woohoo/200.html && surge ./dist/apps/woohoo https://woohoo.surge.sh",
    "start:u-tuber": "ng serve --project=u-tuber --port 4201",
